{
    "name": "angular-typescript-seed-extension",
    "displayName": "angular-typescript-seed-extension",
    "description": "vscode extension for rapid develop in angular typescript seed",
    "version": "0.0.1",
    "publisher": "dgzornoza",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:html"
    ],
    "main": "./out/src/extension",

    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install && typings install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "tslint": "^3.15.1",
        "vscode": "^0.11.0",
        "typings": "^1.3.3"
    }
}